{
	"name": "react-icon-library",
	"version": "0.1.0",
	"private": false,
	"description": "A library of SVG icons for use in reactJS projects",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"bulma": "^0.7.5"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"build-npm": "rm -rf dist && NODE_ENV=production babel src/components --out-dir dist --copy-files",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.5.5",
		"@babel/core": "^7.5.5",
		"husky": "^2.7.0",
		"lint-staged": "^8.2.1",
		"prettier": "^1.18.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-scripts": "3.0.1",
		"stylelint": "^10.1.0",
		"stylelint-order": "^3.0.0",
		"stylelint-scss": "^3.8.0"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --single-quote --write",
			"stylelint --fix",
			"git add"
		]
	},
	"author": "Tommy Scribble",
	"email": "tommy@scribbledesign.co.uk",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TommyScribble/icon-library"
	},
	"homepage": "https://github.com/TommyScribble/icon-library#readme",
	"main": "lib/components/ScribbleIcons.js",
	"keywords": [
		"icon",
		"svg",
		"react",
		"components",
		"library"
	]
}
